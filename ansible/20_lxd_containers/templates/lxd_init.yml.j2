config:
  core.https_address: ":8443"
networks:
- config:
    ipv4.address: 192.168.100.1/24
    ipv6.address: auto
  description: "Default LXD managed network bridge"
  name: lxdbr0
  type: bridge
storage_pools:
- config:
    size: 100GB
  description: "Default storage pool"
  name: default
  driver: {{ lxd_storage_driver }}
profiles:
- config: {}
  description: "Default LXD profile"
  devices:
    eth0:
      name: eth0
      nictype: bridged
      parent: lxdbr0
      type: nic
    root:
      path: /
      pool: default
      type: disk
  name: default